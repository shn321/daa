n = int(input("How many numbers? "))
a = []

print("Enter numbers:")
for i in range(n):
    a.append(int(input()))

for i in range(n):
    min_index = i
    for j in range(i + 1, n):
        if a[j] < a[min_index]:
            min_index = j
    a[i], a[min_index] = a[min_index], a[i]

print("Sorted list:", a)
